{
  "type": "vless",
  "server": { "$ref": "proxy:host" },
  "server_port": { "$ref": "proxy:port" },
  "uuid": { "$ref": "vars:/uuid" },
  "flow": "xtls-rprx-vision",
  "tls": {
    "enabled": true,
    "server_name": "127.0.0.1",
    "certificate": { "$ref": "vars:/server_certificate" },
    "utls": {
      "enabled": true,
      "fingerprint": "firefox"
    }
  }
}
